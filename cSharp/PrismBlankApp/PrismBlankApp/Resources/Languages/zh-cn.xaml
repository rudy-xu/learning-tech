<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:sys="clr-namespace:System;assembly=mscorlib">
  <sys:String
    x:Key="language">语言</sys:String>
  <sys:String
    x:Key="welcome">欢迎使用</sys:String>
  <sys:String
    x:Key="login">请登录</sys:String>
  <sys:String
    x:Key="account">账户:</sys:String>
  <sys:String
    x:Key="password">密码:</sys:String>
  <sys:String
    x:Key="submit">登录</sys:String>
  <sys:String
    x:Key="openModuleA">打开模块A</sys:String>
  <sys:String
    x:Key="openModuleB">打开模块B</sys:String>
  <sys:String
    x:Key="openModuleC">打开模块C</sys:String>
  <sys:String
    x:Key="openModuleApp">打开App</sys:String>
  <sys:String
    x:Key="showDialog">展示弹窗</sys:String>
  <sys:String
    x:Key="return">返回</sys:String>
</ResourceDictionary>
<Window x:Class="PrismBlankApp.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="625" >
  <Grid>
    <Grid.RowDefinitions>
      <RowDefinition Height="auto" />
      <RowDefinition />
    </Grid.RowDefinitions>

    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
      <Button
        Margin="5"
        Command="{Binding OpenCommand}"
        CommandParameter="CutomeName_View"
        Content="Open Module A"/>
      <Button
        Margin="5"
        Command="{Binding OpenCommand}"
        CommandParameter="ViewB"
        Content="Open Module B"/>
      <Button
        Margin="5"
        Command="{Binding OpenCommand}"
        CommandParameter="ViewC"
        Content="Open Module C"/>
      <Button
        Margin="5"
        Command="{Binding OpenCommand}"
        CommandParameter="ModuleApp"
        Content="Open Module App"/>
      <Button
        Margin="5"
        Command="{Binding ShowCommand}"
        CommandParameter="MyDialog"
        Content="Show My Dialog"/>
      <Button
        Margin="5"
        Command="{Binding BackCommand}"
        Content="Return"/>
    </StackPanel>
    <ContentControl Grid.Row="1" prism:RegionManager.RegionName="ContentRegion" />
  </Grid>
</Window>
